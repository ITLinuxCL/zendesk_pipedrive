<ul class="options user_data note">
  <li class="person">
    <b><a target="_blank" href="https://app.pipedrive.com/person/details/{{id}}">{{ name }} </a></b>
  </li>
  {{#if org_name }}
	  <li>{{ org_name }}</li>
  {{/if}}
  <li class="email"><a href="mailto:{{ email }}">{{ email }}</a></li>
  {{#if phone}}
	  <li class="phone">
		  <b>{{ phone }}</b>
	  </li>
  {{/if}}
</ul>
